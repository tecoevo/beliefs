  3.773650432995479*^9}},ExpressionUUID->"56471072-cc3b-4486-a3f8-\
90d3bc60fd63"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"avgpayoffsimplified", "[", 
    RowBox[{"focal1_", ",", "focal2_", ",", "state_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"\[Pi]H", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"Phare1", "[", "focal1", "]"}], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", "0"}], 
          RowBox[{"G", "-", "1"}]], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Binomial", "[", 
            RowBox[{
             RowBox[{"G", "-", "1"}], ",", "k"}], "]"}], " ", 
           SuperscriptBox[
            RowBox[{"(", 
             SubscriptBox["x", "1"], ")"}], "k"], 
           SuperscriptBox[
            RowBox[{"(", 
             SubscriptBox["x", "2"], ")"}], 
            RowBox[{"G", "-", "1", "-", "k"}]], 
           RowBox[{"f1", "[", 
            RowBox[{"k", ",", "state"}], "]"}]}], ")"}]}]}], "+", 
       RowBox[{
        RowBox[{"Phare1", "[", "focal2", "]"}], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", "0"}], 
          RowBox[{"G", "-", "1"}]], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Binomial", "[", 
            RowBox[{
             RowBox[{"G", "-", "1"}], ",", "k"}], "]"}], " ", 
           SuperscriptBox[
            RowBox[{"(", 
             SubscriptBox["x", "1"], ")"}], "k"], 
           SuperscriptBox[
            RowBox[{"(", 
             SubscriptBox["x", "2"], ")"}], 
            RowBox[{"G", "-", "1", "-", "k"}]], 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"f1", "[", 
              RowBox[{"k", ",", "state"}], "]"}]}], ")"}]}], ")"}]}]}]}], 
      ")"}]}], "+", 
    RowBox[{"\[Pi]S", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Phare1", "[", "focal1", "]"}]}], ")"}], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", "0"}], 
          RowBox[{"G", "-", "1"}]], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Binomial", "[", 
            RowBox[{
             RowBox[{"G", "-", "1"}], ",", "k"}], "]"}], " ", 
           RowBox[{"f1", "[", 
            RowBox[{"k", ",", "state"}], "]"}], " ", 
           RowBox[{"Pcomposition", "[", 
            RowBox[{"k", ",", "1"}], "]"}]}], ")"}]}]}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Phare1", "[", "focal2", "]"}]}], ")"}], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"k", "=", "0"}], 
          RowBox[{"G", "-", "1"}]], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Binomial", "[", 
            RowBox[{
             RowBox[{"G", "-", "1"}], ",", "k"}], "]"}], " ", 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"f1", "[", 
              RowBox[{"k", ",", "state"}], "]"}]}], ")"}], "  ", 
           RowBox[{"Pcomposition", "[", 
            RowBox[{"k", ",", "2"}], "]"}]}], ")"}]}]}]}], ")"}]}]}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.7580357298083057`*^9, 3.758035777182871*^9}, {
