  3.77365040026052*^9}},ExpressionUUID->"7e70239d-696c-4f46-ac32-\
928703f48c9c"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Pcomposition", "[", 
    RowBox[{"k_", ",", "state_"}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"l", "=", "0"}], "k"], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"m", "=", "0"}], "k"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"n", "=", "0"}], "k"], 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"o", "=", "0"}], "k"], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"p", "=", "0"}], 
          RowBox[{"G", "-", "1", "-", "k"}]], 
         RowBox[{
          UnderoverscriptBox["\[Sum]", 
           RowBox[{"q", "=", "0"}], 
           RowBox[{"G", "-", "1", "-", "k"}]], 
          RowBox[{
           UnderoverscriptBox["\[Sum]", 
            RowBox[{"r", "=", "0"}], 
            RowBox[{"G", "-", "1", "-", "k"}]], 
           RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{"s", "=", "0"}], 
             RowBox[{"G", "-", "1", "-", "k"}]], 
            RowBox[{"(", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"l", "+", "m", "+", "n", "+", "o"}], "\[Equal]", 
                 "k"}], "&&", 
                RowBox[{
                 RowBox[{"p", "+", "q", "+", "r", "+", "s"}], "\[Equal]", 
                 RowBox[{"G", "-", "1", "-", "k"}]}]}], ",", 
               RowBox[{
                RowBox[{"Multinomial", "[", 
                 RowBox[{"l", ",", "m", ",", "n", ",", "o"}], "]"}], " ", 
                RowBox[{"Multinomial", "[", 
                 RowBox[{"p", ",", "q", ",", "r", ",", "s"}], "]"}], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "hh1"], ")"}], "l"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "hs1"], ")"}], "m"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "sh1"], ")"}], "n"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "ss1"], ")"}], "o"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "hh2"], ")"}], "p"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "hs2"], ")"}], "q"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "sh2"], ")"}], "r"], 
                SuperscriptBox[
                 RowBox[{"(", 
                  SubscriptBox["x", "ss2"], ")"}], "s"], " ", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{"state", "\[Equal]", "1"}], ",", 
                  RowBox[{"HeavisideTheta", "[", 
                   RowBox[{
                   "1", "+", "n", "+", "o", "+", "r", "+", "s", "-", "M"}], 
                   "]"}], ",", 
                  RowBox[{"If", "[", 
                   RowBox[{
                    RowBox[{"state", "\[Equal]", "2"}], ",", 
                    RowBox[{"HeavisideTheta", "[", 
                    RowBox[{
                    "1", "+", "m", "+", "o", "+", "q", "+", "s", "-", "M"}], 
                    "]"}], ",", "0"}], "]"}]}], "]"}]}], ",", "0"}], "]"}], 
             ")"}]}]}]}]}]}]}]}]}], ")"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.7517946988622847`*^9, 3.751794815982048*^9}, {
