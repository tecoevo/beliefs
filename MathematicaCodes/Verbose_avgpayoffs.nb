                "freqnotation", "\[LeftDoubleBracket]", "i", 
                 "\[RightDoubleBracket]"}], "<", "1"}], "&&", 
               RowBox[{"0", "\[Equal]", 
                RowBox[{
                 RowBox[{
                 "freqnotation", "\[LeftDoubleBracket]", "i", 
                  "\[RightDoubleBracket]"}], 
                 RowBox[{"(", 
                  RowBox[{"1", "-", 
                   RowBox[{
                   "freqnotation", "\[LeftDoubleBracket]", "i", 
                    "\[RightDoubleBracket]"}]}], ")"}], 
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"fullreducedmatrix", "\[LeftDoubleBracket]", 
                    RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "-", 
                   RowBox[{"fullreducedmatrix", "\[LeftDoubleBracket]", 
                    RowBox[{"j", ",", "i"}], "\[RightDoubleBracket]"}]}], 
                  ")"}]}]}]}], "/.", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{
                "freqnotation", "\[LeftDoubleBracket]", "j", 
                 "\[RightDoubleBracket]"}], "\[Rule]", 
                RowBox[{"1", "-", 
                 RowBox[{
                 "freqnotation", "\[LeftDoubleBracket]", "i", 
                  "\[RightDoubleBracket]"}]}]}], "}"}]}], ",", 
             RowBox[{
             "freqnotation", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}], ",", "Reals"}], "]"}]}], "//", "N"}],
          ","}], "]"}], "//", "Flatten"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "7", ",", "1"}], "}"}], ",", 
      RowBox[{"{", 
